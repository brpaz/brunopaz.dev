---
import { Image } from 'astro:assets';
const assetsBaseURL = import.meta.env.PUBLIC_ASSETS_BASE_URL || "";

interface Props {
  name: string;
  alt: string;
}
const { name, alt } = Astro.props as Props;
---

<Image
  src={`${assetsBaseURL}/img/blog/${name}`}
  alt={alt}
  width="800"
  height="400"
  title={alt} />
